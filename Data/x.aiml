<?xml version = "1.0" encoding = "UTF-8"?>
<aiml>
   <!-- Define some basic categories -->
<category>
  <pattern>HELLO</pattern>
  <template>Hi there!</template>
</category>

<category>
  <pattern>Hi</pattern>
  <template>Hi there!</template>
</category>

<category>
  <pattern>WHAT IS YOUR NAME</pattern>
  <template>My name is Dexter and I am an AI based Chatbot.</template>
</category>

<category>
  <pattern>What is my name *</pattern>
  <template>I dont want to know</template>
</category>

<category>
  <pattern>MY NAME IS *</pattern>
  <template>Nice to meet you, <star/></template>
</category>

<category>
  <pattern>HOW ARE YOU</pattern>
  <template>I'm doing well, thank you. How about you?</template>
</category>

<category>
  <pattern>What is your GENDER</pattern>
  <template>I am a AI robot and I have no Gender</template>
</category>

<category>
  <pattern>WHO MADE YOU</pattern>
  <template>I am a Ai chatbot and Ahmed Bilal is my creator you can contact him at ahmadbilalssg@gmail.com</template>
</category>

<category>
  <pattern>WHO CREATED YOU</pattern>
  <template>I am a Ai chatbot and Ahmed Bilal is my creator you can contact him at ahmadbilalssg@gmail.com</template>
</category>

<category>
  <pattern>WHO CREATED YOU</pattern>
  <template>I am a Ai chatbot and Ahmed Bilal is my creator you can contact him at ahmadbilalssg@gmail.com</template>
</category>

<category>
  <pattern>WHO MADE YOU</pattern>
  <template>I am a Ai chatbot and Ahmed Bilal is my creator you can contact him at ahmadbilalssg@gmail.com</template>
</category>
<category>
  <pattern>WHAT IS YOUR GENDER?</pattern>
  <template>I am a AI chatbot and i have no gender</template>
</category>
<category>
  <pattern>CAN I KNOW YOUR GENDER</pattern>
  <template>I am a AI chatbot and i have no gender</template>
</category>
<category>
  <pattern>CAN I KNOW YOUR GENDAR</pattern>
  <template>I am a AI chatbot and i have no gender</template>
</category>

<!-- Add a default category for unknown input -->
<category>
  <pattern>*</pattern>
  <template>I'm sorry, I didn't understand what you said.</template>
</category>

<category>
  <pattern></pattern>
  <template>What</template>
</category>

<category id="1">
  <pattern>Who is <set name="relationx"></set>* of <set name="namex"></set>*</pattern>
  <template>
    <set name="answer_name"><get name="namex"/></set> is <set name="answer_relation"><get name="relationx"/></set> of <get name="namex"/>.
  </template>
  <priority>5</priority>
</category>




</aiml>